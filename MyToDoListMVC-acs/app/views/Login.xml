<Alloy>
	<Window id="logincontainer" title="Cloud Login" class="logincontainer">
                <View id='wrapper'>
                        <ImageView image="/images/login.png" id='img'/>
                        <TextField id='username' hintText='username'/>
                        <TextField id='password' hintText='password'/>
                        <Button id='usercreate' onClick="userCreate"/>
                        <Button id='login' onClick="login"/>
                </View>
        </Window>

</Alloy>